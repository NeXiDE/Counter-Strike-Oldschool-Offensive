"VertexlitGeneric"
{
	$basetexture models\props\de_tvstation\studio_spotlight_off_color

	$envmap env_cubemap
	$envmapmask models\props\de_tvstation\studio_spotlight_mask
	$envmaptint "[1 1 1]"
	$envmapcontrast "1.2"
	$envmapsaturation ".4"

	GPU<2
	{
		"$phong" "0"
		"$envmap" ""
		"$envmaptint" "[0 0 0]"
	}
}