VertexlitGeneric
{
	$vertexfog 1
	$treeSway			"1"	// height at which the effect is on fully (in world units)
	$treeSwayHeight			"300"	// radius at which the effect is on fully (in world units)
	$treeSwayStartHeight		"0.45"	// portion of height at which the effect starts to fade in (0-1)
	$treeSwayRadius			"150"	// radius at which the effect is on fully (in world units)
	$treeSwayStartRadius		"0.25"	// portion of radius at which the effect starts to fade in (0-1)
	$treeSwaySpeed			"0.7"	// how quickly the tree sways (as a multiplier of time)
	$treeSwayStrength		"0.9"	// how much the tree sways (as a multiplier of position)
	$treeSwayScrumbleSpeed		"8"	// how quickly the leaves move (as a multiplier of time)
	$treeSwayScrumbleStrength	"0.04"	// how much the leaves move( as a multiplier of position)
	$treeSwayScrumbleFrequency	"1"	// spatial frequency of sine wave applied to leaves.  Typically high-ish (10-25)
	$treeSwayFalloffExp		"1.5"	// falloff parameter for wave motion on branches/trunk, higher means core of tree is more stable
	$treeSwayScrumbleFalloffExp	"3"	// falloff parameter for scrumble motion on leaves, higher means the core of the tree is more stable
	$treeSwaySpeedHighWindMultiplier "1.6"  // multiplier of movement speed at higher wind
	$treeSwaySpeedLerpStart 	"2.0"	// wind speed at which high wind multiplier begins to have effect
	$treeSwaySpeedLerpEnd 		"6.0"	// wind speed at which high wind multiplier is fully on
	GPU<2
	{
		"$phong" "0"
		"$envmap" ""
		"$envmaptint" "[0 0 0]"
	}
}