"vertexlitGeneric"
{
	"$baseTexture" "models/props_misc/fairground_tents"
	"$surfaceprop" "metal"
	"$model"        "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[ .5 .5 .5 ]"

	"$basealphaenvmapmask" "1"
	"$basealphaenvmapmaskMinMaxExp" "[0 0.8 5]"		// munge the alpha channel to make it useable as a specular mask

	"$envmapfresnel" "1"
	"$envmapFresnelMinMaxExp" "[.1 2 3]"
	"$blendtintbybasealpha" 1
	GPU<2
	{
		"$phong" "0"
		"$envmap" ""
		"$envmaptint" "[0 0 0]"
	}
}